<dt><%= field_text %></dt>
<% if changes.present? %>
  <% before, after = changes %>
  <dd>
    <div class='col-md-6'>
      <i>Was</i>
      <%= before.respond_to?(:emtpy?) ? simple_format(before) : before %>
    </div>
    <div class='col-md-6'>
      <i>Now</i><br>
      <%= before.respond_to?(:emtpy?) ? simple_format(after) : after %>
    </div>
    <div class='clearfix'></div>
<% else %>
  <dd><%= field_value %></dd>
<% end %>
